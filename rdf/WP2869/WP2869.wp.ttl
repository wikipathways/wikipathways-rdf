        wp:isAbout           <http://rdf.wikipathways.org/Pathway/WP2869/WP2869_rWP2869_r110881>;
        wp:organismName      "Saccharomyces cerevisiae";
        foaf:page            <http://www.wikipathways.org/instance/WP2869/WP2869_rWP2869_r110881> .

